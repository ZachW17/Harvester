using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using Microsoft.Xna.Framework;
using Microsoft.Xna.Framework.Graphics;

namespace Harvester
{
    //Contains the code neccessary to manipulate background objects.
    //@author Derrick Hunt
    //@version 1.0
    //@date 4/1/12

    //REVISIONS
    //1. took out the Draw method and added it to the abstract game object class

    public class Background : GameObject
    {
        //constructor
        public Background(Rectangle rect, Texture2D tex) : base(rect, tex)
        {
            objectPosition = rect;
            objectTexture = tex;
        }
    }
}
