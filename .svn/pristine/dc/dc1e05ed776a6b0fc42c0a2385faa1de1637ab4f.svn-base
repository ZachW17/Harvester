using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using Microsoft.Xna.Framework;
using Microsoft.Xna.Framework.Graphics;

namespace Harvester
{
    /// <summary>
    /// Represents the backgrounds that will be displayed duringthe 
    /// </summary>
    public class Background : GameObject
    {
        //constructor
        public Background(Rectangle rect, Texture2D tex) : base(rect, tex)
        {
            objectPosition = rect;
            objectTexture = tex;
        }
    }
}
